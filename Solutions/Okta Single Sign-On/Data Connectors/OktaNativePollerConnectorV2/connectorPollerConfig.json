[
    {
        "name": "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/', 'OktaDCV1')]",
        "apiVersion": "2022-12-01-preview",
        "type": "Microsoft.OperationalInsights/workspaces/providers/dataConnectors",
        "location": "[parameters('workspace-location')]",
        "kind": "RestApiPoller",
        "properties": {
            "dataType": "Okta System Log API",
            "response": {
                "eventsJsonPaths": [
                    "$"
                ],
                "format": "json"
            },
            "paging": {
                "pagingType": "LinkHeader"
            },
            "connectorDefinitionName": "[[parameters('connectorDefinitionName')]",
            "auth": {
                "apiKeyName": "Authorization",
                "ApiKey": "[[parameters('apikey')]",
                "apiKeyIdentifier": "SSWS",
                "type": "APIKey"
            },
            "request": {
                "apiEndpoint": "[[concat('https://',parameters('domainname'),'/api/v1/logs')]",
                "rateLimitQPS": 10,
                "queryWindowInMin": 5,
                "httpMethod": "GET",
                "retryCount": 3,
                "timeoutInSeconds": 60,
                "headers": {
                    "Accept": "application/json",
                    "User-Agent": "Scuba"
                },
                "startTimeAttributeName": "since",
                "endTimeAttributeName": "until"
            },
            "dcrConfig": {
                "dataCollectionEndpoint": "[[parameters('dcrConfig').dataCollectionEndpoint]",
                "dataCollectionRuleImmutableId": "[[parameters('dcrConfig').dataCollectionRuleImmutableId]",
                "streamName": "Custom-OktaSSO_CL"
            },
            "isActive": true
        }
    }
]