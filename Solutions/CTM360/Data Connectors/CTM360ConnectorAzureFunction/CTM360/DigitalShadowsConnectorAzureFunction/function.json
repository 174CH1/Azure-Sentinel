{
    "scriptFile": "__init__.py",
    "bindings": [
      {
        "name": "mytimer",
        "type": "timerTrigger",
        "direction": "in",
        "schedule": "0 */5 * * * *"
      },
      {
        "name": "inputblob",
        "type": "blob",
        "dataType": "string",
        "path": "ttss/data/flag.txt",
        "connection": "AzureWebJobsStorage",
        "direction": "in"
      },
      {
        "name": "outputblob",
        "type": "blob",
        "dataType": "string",
        "path": "ttss/data/flag.txt",
        "connection": "AzureWebJobsStorage",
        "direction": "out"
      }
    ]
  }