      "type": "timerTrigger",
      "direction": "in",
      "schedule": "*/30 * * * * *"
    },
    {
      "name": "inputblob",
      "type": "blob",
      "dataType": "string",
      "path": "ctm360/data/flagHV.txt",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "dataType": "string",
      "path": "ctm360/data/flagHV.txt",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ]
}