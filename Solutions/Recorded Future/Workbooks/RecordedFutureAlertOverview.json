          "version": "KqlItem/1.0",
          "query": "{log_table:value}\n| where RuleName_s in ({alert_rules:value})\n| distinct AlertID_s, RuleName_s, Triggered_t\n| summarize alert_count = count() by RuleName_s\n| project alert_count, Alert = RuleName_s\n",
